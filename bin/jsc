#!/bin/bash

myPATH='/usr/local/bin'
CUR_DIR=$(readlink $myPATH/jsc)
CUR_PATH=$(pwd)

node "$myPATH/$CUR_DIR/../../src/index.js" m="$CUR_PATH/" all=yes
echo '-----------------------'
echo '|       have fun      |'
echo '-----------------------'
node "$myPATH/$CUR_DIR/../../src/index.js" m="$CUR_PATH/"
