.resetBor .after {
  opacity: 0;
}
.tBor {
  position: relative;
}
.tBor .before {
  position: absolute;
  content: "";
  height: 1px;
  background: #a2a2a0;
  left: 0;
  right: 0;
  top: 0;
}
.bBor:after{
  position: absolute;
  content: "";
  height: 1px;
  background: #cbcbcb;
  left: 0;
  right: 0;
  bottom: 0;
}
.bBor {
  position: relative;
}
.bBor .after {
  position: absolute;
  content: "";
  height: 1px;
  background: #a2a2a0;
  left: 0;
  right: 0;
  bottom: 0;
}
.lBor {
  position: relative;
}
.lBor .before {
  position: absolute;
  content: "";
  width: 1px;
  background: #a2a2a0;
  top: 0;
  bottom: 0;
  left: 0;
}
.rBor {
  position: relative;
}
.rBor .after {
  position: absolute;
  content: "";
  width: 1px;
  background: #a2a2a0;
  top: 0;
  bottom: 0;
  right: 0;
}
.trblBor {
  position: relative;
}
.trblBor .after {
  position: absolute;
  content: "";
  top: 0;
  left: 0;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  border: 1px solid #a2a2a0;
  point-events: none;
}

